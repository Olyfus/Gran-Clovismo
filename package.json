{
  "name": "gran-clovismo",
  "version": "0.3.7",
  "scripts": {
    "ng": "ng",
    "start:local": "npm run update-patch && ng serve",
    "start:dev": "npm run update-patch && npm i && ng serve",
    "start:lan": "npm i && ng serve -host 0.0.0.0 -port 4200 --watch",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "update-major": "npm --no-git-tag-version version major",
    "update-minor": "npm --no-git-tag-version version minor",
    "update-patch": "npm --no-git-tag-version version patch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.4",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@mapbox/mapbox-sdk": "^0.16.0",
    "@types/leaflet": "^1.9.12",
    "axios": "^1.7.2",
    "leaflet": "^1.9.4",
    "mapbox-gl": "^3.4.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular/cli": "^17.0.3",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.12.12",
    "autoprefixer": "^10.4.19",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.2.2"
  }
}
